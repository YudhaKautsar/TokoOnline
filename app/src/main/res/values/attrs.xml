        <attr name="progressColor" format="color" />
        <attr name="backgroundColorProgress" format="color" />
        <attr name="separatorColor" format="color" />
        <attr name="strokeWidth" format="dimension" />
        <attr name="separatorAngle" format="float" />
        <attr name="progress" format="integer" />
        <attr name="maxProgress" format="integer" />
    </declare-styleable>-->
    <declare-styleable name="CircularProBar">
        <attr name="usedColor" format="color" />
        <attr name="remainingColor" format="color" />
        <attr name="separatorColor" format="color" />
        <attr name="separatorThickness" format="dimension" />
        <attr name="progress" format="float" />
        <attr name="maxProgress" format="float" />
    </declare-styleable>

</resources>