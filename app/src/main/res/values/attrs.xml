<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="InputEditText">
        <attr name="android:inputType" />
        <attr name="android:textAppearance" />
        <attr name="android:drawableRight" />
        <attr name="android:focusable" />
        <attr name="ietCounterEnabled" format="boolean" />
        <attr name="ietCounterMaxLength" format="integer" />
        <attr name="ietErrorEnabled" format="boolean" />
        <attr name="ietHeight" format="dimension">
            <enum name="fill_parent" value="-1" />
            <enum name="match_parent" value="-1" />
            <enum name="wrap_content" value="-2" />
        </attr>
        <attr name="ietHintEnabled" format="boolean" />
        <attr name="ietTextSize" format="dimension" />
        <attr name="ietText" format="string" />
        <attr name="ietHint" format="string" />
        <attr name="ietError" format="string" />
        <attr name="ietLines" format="integer" />
        <attr name="ietMaxLines" format="integer" />
        <attr name="android:imeOptions" />
        <attr name="showBottomLine" format="boolean" />
        <attr name="showPrefix" format="boolean" />
        <attr name="ietEnable" format="boolean" />
        <attr name="ietClickEnable" format="boolean" />
        <attr name="ietPasswordToggleEnabled" format="boolean" />
        <attr name="ietFocusEnable" format="boolean" />
        <attr name="ietMinLength" format="integer" />
        <attr name="ietMaxLength" format="integer" />
        <attr name="ietDescription" format="boolean" />
        <attr name="ietSubtitle" format="string" />
        <attr name="ietRightLabel" format="string" />
        <attr name="ietHorizontal" format="boolean" />
        <attr name="ietSubStyle" format="enum">
            <enum name="primary" value="1" />
            <enum name="secondary" value="2" />
            <enum name="ternary" value="3" />
            <enum name="quaternary" value="4" />
            <enum name="quinary" value="5" />
            <enum name="senary" value="6" />
            <enum name="septenary" value="7" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ButtonLoading">
        <attr name="android:text" />
        <attr name="isLoading" format="boolean" />
        <attr name="isClickAble" format="boolean" />
        <attr name="loadIcon" format="reference" />
        <attr name="ietStyle" format="enum">
            <enum name="primary" value="1" />
            <enum name="secondary" value="2" />
            <enum name="ternary" value="3" />
            <enum name="quaternary" value="4" />
            <enum name="quinary" value="5" />
            <enum name="senary" value="6" />
            <enum name="septenary" value="7" />
            <enum name="centenary" value="8" />

        </attr>
    </declare-styleable>

    <declare-styleable name="IconTextMenuView">
        <attr name="ictmText" format="string" />
        <attr name="ictmIcon" format="reference" />
        <attr name="ictmSubIcon" format="reference" />
        <attr name="ictmSubtitle" format="string" />
        <attr name="ictmShowIcon" format="boolean" />
        <attr name="ictmRightIcon" format="reference" />
        <attr name="ictmVisibleIconArrow" format="boolean" />
        <attr name="ictmVisibleIconCopy" format="boolean" />
        <attr name="ictmVisibleIconMap" format="boolean" />
        <attr name="ictmVisibleIconPhone" format="boolean" />
        <attr name="ictmVisibleIconWhatsapp" format="boolean" />
        <attr name="ictmAlignEnd" format="boolean" />
        <attr name="ictmStyle" format="enum">
            <enum name="primary" value="1" />
            <enum name="secondary" value="2" />
            <enum name="ternary" value="3" />
            <enum name="quaternary" value="4" />
            <enum name="quinary" value="5" />
            <enum name="senary" value="6" />
            <enum name="septenary" value="7" />
        </attr>
        <attr name="ietStyleList" format="enum">
            <enum name="primary" value="1" />
            <enum name="secondary" value="2" />
            <enum name="ternary" value="3" />
            <enum name="quaternary" value="4" />
            <enum name="quinary" value="5" />
            <enum name="senary" value="6" />
        </attr>
    </declare-styleable>


    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_circle_background_color" format="color" />
    </declare-styleable>


    <declare-styleable name="CircleNumber">
        <attr name="circleText" format="string" />
        <attr name="circleBackground" format="string" />
    </declare-styleable>

    <declare-styleable name="LegendDashboard">
        <attr name="legText" format="string" />
        <attr name="legCount" format="string" />
        <attr name="legColorTint" format="string" />
        <attr name="legIcon" format="reference" />
    </declare-styleable>

    <declare-styleable name="CollapsableTitleContent">
        <attr name="collapsableStyle" format="enum">
            <enum name="primary" value="1" />
            <enum name="secondary" value="2" />
            <enum name="ternary" value="3" />
            <enum name="quaternary" value="4" />
            <enum name="quinary" value="5" />
            <enum name="senary" value="6" />
            <enum name="septenary" value="7" />
        </attr>

        <attr name="isCollapsableEnable" format="boolean"/>
        <attr name="collapsableView" format="reference"/>
        <attr name="callsapsableInitialState" format="enum">
            <enum name="collapse" value="0"/>
            <enum name="expand" value="1"/>
        </attr>
        <attr name="collapsableTitle" format="string"/>
        <attr name="collapsableSubtitle" format="string"/>
        <attr name="isAddEnabled" format="boolean"/>
    </declare-styleable>



    <declare-styleable name="IconSwitch">
        <attr name="isw_icon_left" format="reference" />
        <attr name="isw_icon_right" format="reference" />

        <attr name="isw_icon_size" format="dimension" />

        <attr name="isw_background_color" format="color" />
        <attr name="isw_inactive_background_color" format="color" />
        <attr name="isw_thumb_color_left" format="color" />
        <attr name="isw_thumb_color_right" format="color" />
        <attr name="isw_inactive_tint_icon_left" format="color" />
        <attr name="isw_inactive_tint_icon_right" format="color" />
        <attr name="isw_active_tint_icon_left" format="color" />
        <attr name="isw_active_tint_icon_right" format="color" />

        <attr name="isw_default_selection" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>

    </declare-styleable>

    <attr name="refCustomPinEntrySelected" format="reference" />
    <attr name="refCustomPinEntryFocus" format="reference" />
    <attr name="refCustomPinEntryUnFocus" format="reference" />

